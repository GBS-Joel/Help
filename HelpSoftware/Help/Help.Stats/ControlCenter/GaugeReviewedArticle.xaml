<UserControl x:Class="Help.Stats.GaugeReviewedArticle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             xmlns:local="clr-namespace:Help.Stats"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance  local:MainWindow}"  MinHeight="150" MinWidth="150">
  <Grid>
    <lvc:Gauge Grid.Row="0" Grid.Column="0" Margin="5" x:Name="test" UseLayoutRounding="False" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
                   Uses360Mode="False" From="0" To="100" Value="50" />
  </Grid>
</UserControl>