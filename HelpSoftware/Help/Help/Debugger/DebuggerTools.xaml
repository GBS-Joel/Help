<UserControl x:Class="Help.Debugger.DebuggerTools"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Help.Debugger"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
  <WrapPanel>
    <Button Width="200" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Content="Reset Permission" />
    <Button Width="200" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Content="Reload Context" />
    <Button x:Name="btnRunGarbage" Width="200" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Content="Run Garbace Colleciton" Click="btnRunGarbage_Click" />
    <Label VerticalAlignment="Top" HorizontalAlignment="Left" Content="Launch Window" />
    
    <ComboBox VerticalAlignment="Top" HorizontalAlignment="Left" Width="200" x:Name="ItemsWindow"/>
    <Button VerticalAlignment="Top" HorizontalAlignment="Left" Content="Launch"  Click="Button_Click"/>
  </WrapPanel>
</UserControl>